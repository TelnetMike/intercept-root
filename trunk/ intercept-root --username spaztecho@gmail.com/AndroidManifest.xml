<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	android:versionName="1.0.0"
	android:versionCode="6" package="com.intercept.root">
	<uses-permission android:name="android.permission.WAKE_LOCK" />
	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
	<uses-permission android:name="android.permission.READ_LOGS" /> 
	<uses-permission android:name="android.permission.GET_TASKS" />
	<uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
	<application android:icon="@drawable/logo"
		android:label="Intercept Root" android:debuggable="true">
		<activity android:name="Root" android:theme="@style/Theme" android:screenOrientation="portrait" android:configChanges="keyboard|keyboardHidden|orientation">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	</application>
	<uses-sdk android:minSdkVersion="8" android:targetSdkVersion="8"/>
</manifest>
